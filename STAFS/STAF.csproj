<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageId>STAF.UI.API</PackageId>
    <Authors>Sooraj Ramachandran</Authors>
    <Product>STAF</Product>
    <Description>Test Automation Framework for Selenium UI and API Tests with inbuild reporting capabilities</Description>
    <Copyright></Copyright>
    <AssemblyName>STAF</AssemblyName>
    <RootNamespace>STAF</RootNamespace>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>Selenium, Framework, API, UI Tests, API Tests</PackageTags>
    <PackageReleaseNotes>UI and API test framework for Selenium.
Supports Chrome, Edge &amp; Remote Drivers.
Supports Parallel execution and is thread safe.
Install STAFTemplate Test automation framework from Microsoft Market place. (From VS2019 -&gt; Extension -&gt; Manage Extension -&gt; Online Templates -&gt; Search "STAFTemplate"</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/sooraj171</RepositoryUrl>
    <PackageIcon>test - Copy11.png</PackageIcon>
    <PackageIconUrl />
    <Version>2.0.2</Version>
    <AssemblyVersion>2.0.0.2</AssemblyVersion>
    <FileVersion>2.0.0.2</FileVersion>
    <RepositoryType />
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DotNetSeleniumExtras.WaitHelpers" Version="3.11.0" />
    <PackageReference Include="MSTest.TestFramework" Version="3.0.2" />
    <PackageReference Include="Selenium.Support" Version="4.7.0" />
    <PackageReference Include="Selenium.WebDriver" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <None Update="test - Copy11.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>

<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>STAF.UI.API</PackageId>
    <Authors>Sooraj Ramachandran</Authors>
    <Product>STAF</Product>
    <Description>Test Automation Framework for Selenium UI and API Tests with inbuild reporting capabilities</Description>
    <Copyright></Copyright>
    <AssemblyName>STAF</AssemblyName>
    <RootNamespace>STAF</RootNamespace>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>Selenium, Framework, API, UI Tests, API Tests</PackageTags>
    <PackageReleaseNotes>UI and API test framework for Selenium.
Supports Chrome, Edge &amp; Remote Drivers.
Supports Parallel execution and is thread safe.</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/sooraj171/TestAutomation</RepositoryUrl>
    <PackageIcon>test - Copy11.png</PackageIcon>
    <PackageIconUrl />
    <Version>1.0.1</Version>
    <AssemblyVersion>1.0.1.1</AssemblyVersion>
    <FileVersion>1.0.1.1</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotNetSeleniumExtras.WaitHelpers" Version="3.11.0" />
    <PackageReference Include="MSTest.TestFramework" Version="2.2.8" />
    <PackageReference Include="Selenium.Support" Version="4.1.0" />
    <PackageReference Include="Selenium.WebDriver" Version="4.1.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Downloads\test - Copy11.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
